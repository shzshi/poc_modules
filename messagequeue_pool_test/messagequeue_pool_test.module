/**
 * Implement hook_ctools_plugin_api().
 */
function messagequeue_pool_test_ctools_plugin_api($module, $api) {
  if ($module == 'messagequeue' && $api == 'messagequeue_default_pools') {
    return array('version' => 1);
  }
}

/**
 * Implements hook_menu().
 *
 *
 */
function messagequeue_pool_test_menu(){
  $items['msgqueue/test'] = array(
    'title' => t('test page'),
    'page callback' => array('messagequeu_pool_test_page'),
    'access arguments' => array('access content'),
  );
  return $items;
}

function messagequeu_pool_test_page() {

   $pool = messagequeue_get_test_pool();
   $msg = 'testing'; // Custom message format.
   $pool->send('/queue/testing', $msg);
   
}
